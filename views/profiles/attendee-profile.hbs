
<section class="section-height attendee-profile">
    <h1>Welcome to Your Profile</h1>
     

    <h3>Your Upcoming Events:</h3>
    
 {{#if isAttending}}
       
    {{#each events}}
  
    <article class="attendee-events">
         <p>Date: {{this.hostId.host.scheduleTime}} </p>
        <p>Location: {{this.hostId.host.address}}, {{this.hostId.host.city}}</p>
          <p>Genre: {{this.artistId.artist.genre}}</p>
    </article>
    
    {{/each}}

  {{else}}
        <p>No event yet</p>
        {{/if}}

   

  
  
   



    <a href="/#events">Check Events</a>
</section>
